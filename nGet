
def nGet(number):
    with open('notes.txt', 'r') as f:
        noteNumbers = [line.split(None, 1)[0] for line in f]
        with open('notes.txt', 'r') as x:
            notes = x.readlines()

    for x in range(len(notes)):
        notes[x] = notes[x].replace(noteNumbers[x], '', 1)
        notes[x] = notes[x].replace(' ', '', 1)
    print(noteNumbers)
    print(notes)
